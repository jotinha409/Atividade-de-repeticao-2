public class Exercicio8 {

    public void executarExercicio8 () {
        int numero;

        System.out.println("Iniciando exercicio 8 -- 20 primeiros números naturais");
        System.out.println("Os 20 primeiros números naturais");

        for ( numero = 0; numero < 20; numero++){
            System.out.println(numero);

        }
    }
}
